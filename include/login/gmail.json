{
	"type": "webform",
		"name": "GMail",
		"address": "gmail.com",

		"form":
		{
			"action": "https://accounts.google.com/ServiceLoginAuth",
			"method": "post",
			"fields":
			{
				"Email": "$username",
				"Passwd": "$password",
				"PersistentCookie": "yes",
				"continue": "https://mail.google.com/mail/",
				"service": "mail"
			}
		}
}
